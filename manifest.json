{
  "manifest_version": 3,
  "name": "Jiocinema IPL admuter",
  "description": "This extension mutes the player as ad starts and unmute as ad ends",
  "version": "1.0",
  "action": {
    "default_popup": "index.html",
    "default_icon": "adIcon.png"
  },
  "content_scripts": [
    {
      "js": ["content.js"],
      "matches": ["https://www.jiocinema.com/*"]
    }
  ],
  "icons": {
    "16": "adIcon.png",
    "32": "adIcon.png",
    "48": "adIcon.png",
    "128": "adIcon.png"
  }
}
